            button>span:after {
                font-family: FontAwesome;
                display: inline-block;
                padding-left: {{settings.btn_style_txt_icon_pd}}px;
                vertical-align: middle;
                content: "\f{{settings.btn_style_txt_icon_type}}";
                color: {{ settings.btn_style_txt_icon_color }};
