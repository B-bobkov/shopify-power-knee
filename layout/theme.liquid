  <title>
  {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
  {% endif %}

  {% include 'social-meta-tags' %}

  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}

  <script>
    window.theme = window.theme || {};
    theme.strings = {
      cartEmpty: {{ 'cart.general.empty' | t | json }},
      savingHtml: {{ 'cart.general.savings_html' | t: price: '[savings]' | json }},
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      addressError: {{ 'map.address_error' | t | json }},
      addressNoResults: {{ 'map.address_no_results' | t | json }},
      addressQueryLimit: {{ 'map.address_query_limit_html' | t | json }},
      authError: {{ 'map.auth_error_html' | t | json }}
    };
    theme.settings = {
      cartType: {{ settings.cart_type | json }},
      moneyFormat: {{ shop.money_format | json }}
    };
  </script>

  <script src="{{ 'lazysizes.min.js' | asset_url }}" async="async"></script>

  {% if request.page_type contains 'customers/' %}
    <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script>
  {% endif %}

  <script src="{{ 'vendor.js' | asset_url }}" defer="defer"></script>

  <script src="{{ 'option_selection.js' | shopify_asset_url }}" defer="defer"></script>

  <script src="{{ 'theme.js' | asset_url }}" defer="defer"></script>

  {{ content_for_header }}
  
  
<style>
  
/* Typography Headline Font */
  h2{
    
    font-size: {{ settings.headline_font_laptop_size_2 }}px;
    font-weight: {{ settings.headline_font_weight }};
    color: {{ settings.headline_fcol }};
    line-height: {{ settings.headline_font_laptop_size_2 | plus: settings.headline_font_line_height }}px;
  }
     @media only screen and (max-width: 600px) {
  	   h2{
    	font-size: {{ settings.headline_font_mob_size_2 }}px;
        line-height: {{ settings.headline_font_mob_size_2 | plus: settings.headline_font_line_height }}px;
  	   }
     }
  
/*   Typography CONTENT FONT */
  p{
    font-size: {{ settings.content_font_laptop_size_2 }}px;
    font-weight: {{ settings.content_font_weight }};
    line-height: {{ settings.content_font_laptop_size_2 | plus: settings.content_font_line_height }}px;
    color: {{ settings.content_fcol }};
  }
     @media only screen and (max-width: 600px) {
  	   p{
    	font-size: {{ settings.content_font_mob_size_2 }}px;
         line-height: {{ settings.content_font_mob_size_2 | plus: settings.content_font_line_height }}px;
  	   }
     }
  
/*   Button styling */
  button, .shopify-payment-button .shopify-payment-button__button--unbranded{
    padding-top: {{ settings.btn_style_pd_top }}px!important;
    padding-bottom: {{ settings.btn_style_pd_bt }}px!important;
    padding-left: {{ settings.btn_style_pd_side }}px!important;
    padding-right: {{ settings.btn_style_pd_side }}px!important;
    border-radius: {{ settings.btn_style_radius }}px!important;  
    box-shadow: {{ settings.btn_style_shadow_coverage }}px {{ settings.btn_style_shadow_coverage }}px 5px {{ settings.btn_style_shadow_color }};
    text-align: {{ settings.btn_style_txt_font_alignment }}!important;

  }
  button:hover{
    background-color: {{ settings.btn_style_color_on_hover }}!important;
    color: {{ settings.btn_style_text_fcol_on_hover }}!important;
  }
  .shopify-payment-button:hover, .shopify-payment-button__button--unbranded:hover{
    color: {{ settings.btn_style_text_fcol_on_hover }}!important;
  }  
  

  /*   Button text style */
  .btn__text, .shopify-payment-button .shopify-payment-button__button--unbranded{

  }
  
  button>span {
    font-family: {{ settings.btn_style_text_font }};
    font-weight: {{ settings.btn_style_font_weight }}!important;
    font-size: {{ settings.btn_style_font_laptop_size }}px;
    line-height: {{ settings.btn_style_font_line_height }};
    color: {{ settings.btn_style_text_fcol }};
  }
  
  {% if settings.btn_style_font_same_on_mob == false %}
    @media only screen and (max-width: 600px) {
      button>span  {
          font-size: {{ settings.btn_style_font_mob_size }}px;
      }
    }
  {% endif %}  
  
  button>span:hover {
     color: {{ settings.btn_style_text_fcol_on_hover }};
  }
  
  {% unless settings.btn_style_font_same_on_mob == true %}
     @media only screen and (max-width: 600px) {
  	.btn__text{
    	font-size: {{ settings.btn_style_font_mob_size }}px;;
  	   }
     }
  {% endunless %} 
  button:hover > span{
    color: {{ settings.btn_style_text_fcol_on_hover }};
   
  }
  
  #slide_cart_header_title {
    margin: 25px 10px;
    font-family: {{ settings.slide_cart_header_title_font_family }};
    {% if settings.slide_cart_header_title_bold == true %}
    	font-weight: bolder;
    {% endif %}
    {% if settings.slide_cart_header_title_italic == true %}
    	font-style: italic;
    {% endif %}    
    {% if settings.slide_cart_header_title_underline == true %}
    	text-decoration: underline;
    {% endif %}
    font-weight: {{ settings.slide_cart_header_title_font_weight }};
    font-size: {{ settings.slide_cart_header_title_fsize }}px;
    color: {{ settings.slide_cart_header_title_fcol }};
  }

  #slide_cart_header_wrapper {
    padding: {{ settings.slide_cart_header_laptop_pd }}px 0;
  }

  @media only screen and (max-width: 600px) {
    #slide_cart_header_wrapper {
      padding: {{ settings.slide_cart_header_mob_pd }}px 0;
    }
    
    {% if settings.slide_cart_header_title_fsize_same_mob == false %}
       #slide_cart_header_title {
    	  font-size: {{ settings.slide_cart_header_title_fsize_mob }}px;
       }
    {% endif %}
    
    {% if settings.slide_cart_announce_headline_fsize_same_mob == false %}
      #slide_cart_annbar_txt {
        font-size: {{ settings.slide_cart_announce_headline_fsize_mob }}px!important;
      }
    {% endif %}
    
    #slide_cart_annbar_txt { 
      padding: {{ settings.slide_cart_announcebar_mob_pd }}px 10px!important;
    }
  }
  
  {% if settings.slide_cart_header_enable_border == true %}
    #slide_cart_header_wrapper { 
      {% if settings.slide_cart_header_border_style == 'full_border' %}		
      	border: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      {% endif %}
      {% if settings.slide_cart_header_border_style == 'top_bt_border' %}
		border-top: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
        border-bottom: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      {% endif %}
      {% if settings.slide_cart_header_border_style == 'top_border_only' %}
		border-top: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      {% endif %}
      {% if settings.slide_cart_header_border_style == 'bottom_border_only' %}
		border-bottom: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      {% endif %}
      {% if settings.slide_cart_header_border_style == 'side_border_only' %}
		border-left: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      	border-right: {{ settings.slide_cart_header_border_thickness }}px solid {{ settings.slide_cart_header_border_color }};
      {% endif %}
    }
  {% endif %}
  
  #slide_cart_annbar { 
    position: absolute;
    background-color: {{ settings.slide_cart_announcebar_bg_color }};
    width: calc(100%);
    top: 68px;
    
    {% if settings.slide_cart_announcebar_enable_border == true %}

      {% if settings.slide_cart_announcebar_border_style == 'full_border' %}		
          border: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
      {% endif %}
      {% if settings.slide_cart_announcebar_border_style == 'top_bt_border' %}
        border-top: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
        border-bottom: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
      {% endif %}
      {% if settings.slide_cart_announcebar_border_style == 'top_border_only' %}
        border-top: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
      {% endif %}
      {% if settings.slide_cart_announcebar_border_style == 'bottom_border_only' %}
        border-bottom: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
      {% endif %}
      {% if settings.slide_cart_announcebar_border_style == 'side_border_only' %}
        border-left: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
          border-right: {{ settings.slide_cart_announcebar_border_thickness }}px solid {{ settings.slide_cart_announcebar_border_color }};
      {% endif %}

    {% endif %}
    
  }
  
  #slide_cart_annbar_txt { 
    margin: 0;
    padding: {{ settings.slide_cart_announcebar_laptop_pd }}px 10px;
    text-align: center;
    font-family: {{ settings.slide_cart_announce_headline_font_family }};
    font-weight: {{ settings.slide_cart_announce_headline_font_weight }};

    {% if settings.slide_cart_announce_headline_italic %}
