{% if section.settings.cart_btn_text_font contains 'Google' %}
<link href="//fonts.googleapis.com/css?family={{ section.settings.cart_btn_text_font | replace: 'Google_', '' | replace: "'", "" }}:light,normal,bold" rel="stylesheet" type="text/css" />
{% endif %}
{% if section.settings.empty_cart_text_font contains 'Google' %}
<link href="//fonts.googleapis.com/css?family={{ section.settings.empty_cart_text_font | replace: 'Google_', '' | replace: "'", "" }}:light,normal,bold" rel="stylesheet" type="text/css" />
{% endif %}

<style>
    #loading_cart_wrapper{
        z-index: 17;
        position: fixed;  
        bottom: 0;
        top: 0;  
        width: 100%; 
        background-image: url({{ "cart_loading.gif" | asset_url }});
        background-repeat: no-repeat;
        background-position: center;
        background-size: 18%;
        background-color: {{ section.settings.slide_cart_bg_color }};
    }

    #cart_blocks_container {
        background-color: {{ section.settings.slide_cart_bg_color }};
    }

    #bt_sticked_blocks {
        background-color: {{ section.settings.slide_cart_bg_color }};
    }

    .drawer-page-content:after{
        background-color: {{ section.settings.non_slide_part_color }}!important;
    }

    @media only screen and (min-width: 600px) {
        #CartDrawer {
            width: {{ section.settings.cart_lap_width }}px!important;
        }
    }

    @media only screen and (max-width: 600px) {
        #CartDrawer {
            {% if section.settings.cart_mob_width == "full_Width" %}
                max-width: 100%!important;
                width: 100%!important;
                {% else %}
                width: 75%!important;                    
            {% endif %}
        }
    }

    .empty_cart_elements_wrapper {
        margin: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        -ms-transform: translate(-50%, -50%);
        transform: translate(-50%, -50%);
        width: 300px;
    }

    #empty_cart_img_wrapper>img{
        width: 30%;
        margin: 0;
        display: block;
        margin-left: auto;
        margin-right: auto;
    }

    #empty_cart_msg {
        text-align: center;
        margin-top: 30px;
        margin-bottom: 15px;
    }

    #empty_cart_msg>p {
        margin: 0!important;
        color: {{ section.settings.empty_cart_text_col }};
        font-family: {{ section.settings.empty_cart_text_font | replace: 'Google_', '' | replace: '+', ' ' }};
        font-weight: {{ section.settings.empty_cart_text_weight }};
        font-size: {{ section.settings.empty_cart_text_lap_fsize }}px;
        line-height: 1!important;
    }

    @media only screen and (max-width: 600px) {
        #empty_cart_msg>p {
            font-size: {{ section.settings.empty_cart_text_mob_fsize }}px;
        }
    }
    
    #empty_cart_shop_btn>button{
        display: block;
        outline: none;
        border: none;
        margin-left: auto;
        margin-right: auto;
        background-color: {{ section.settings.cart_btn_bg_col }};
        border-radius: {{ section.settings.cart_btn_radius }}px;
        {% if section.settings.cart_btn_shadow_enable == true %}
            box-shadow: 0 {{ section.settings.cart_btn_shadow_coverage }}px 3px {{ section.settings.cart_btn_shadow_col }};
        {% endif %}
        padding: 15px 0;
        width: 100%;
    }

    #empty_cart_shop_btn>button:hover{
        background-color: {{ section.settings.cart_btn_col_on_hover }};
    }
    #empty_cart_shop_btn>button:hover>p{
        color:  {{ section.settings.cart_btn_txt_col_on_hover }};
    }

    #empty_cart_shop_btn>button>p{
        margin:0;
        color:  {{ section.settings.cart_btn_txt_col }};
        font-family: {{ section.settings.cart_btn_text_font | replace: 'Google_', '' | replace: '+', ' ' }};
        font-weight: {{ section.settings.cart_btn_text_weight }};
        font-size: {{ section.settings.cart_btn_text_lap_fsize }}px;
    }

    @media only screen and (max-width: 600px) {
        #empty_cart_shop_btn>button>p{
            font-size: {{ section.settings.cart_btn_text_mob_fsize }}px;
        }
    }

    #empty_cart_wrapper {
        background-color: {{ section.settings.slide_cart_bg_color }};
        z-index: 16;
    }

</style>

<input id="preview_empty_cart" type="text" style="position:fixed; top:-100px;" value={{ section.settings.preview_empty_cart }} >

<div id="cart_blocks_container" style="position: fixed; top: 0; overflow: auto; width: 100%;">

    {% for block in section.blocks %}
        <div {{ block.shopify_attributes }}>
            {% case block.type %} 
                {% when 'cart_header' %} 
                {% include 'cart_header' %} 
                
                {% when 'Countdown_Timer' %}
                {% include 'Countdown_Timer' %} 
                
                {% when 'Announcement_Bar' %} 
                {% include 'Announcement_Bar' %}

                {% when 'Free_Shipping_Bar' %} 
                {% include 'Free_Shipping_Bar'%} 

                {% when 'Your_Savings_Announcement' %} 
                {% include 'Your_Savings_Announcement' %} 

                {% when 'Cart_Products' %}
                {% include 'Cart_Products' %} 

                {% when 'Discount_Code' %} 
                {% include 'Discount_Code' %}

                {% when 'slide_cart_button' %} 
                {% include 'slide_cart_button'%} 
                
                {% when 'slide_cart_img' %} 
                {% include 'slide_cart_img' %} 

                {% when 'Shipping_Price_Summary' %}
                {% include 'Shipping_Price_Summary' %} 

                {% when 'Your_Savings_Summary' %} 
                {% include 'Your_Savings_Summary' %}

                {% when 'Total_Price_Summary' %} 
                {% include 'Total_Price_Summary' %} 

                {% when 'Free_Shipping_Motivator' %} 
                {% include 'Free_Shipping_Motivator' %}

                {% when 'Discount_Motivator' %} 
                {% include 'Discount_Motivator' %} 

                {% when 'slide_cart_Divider' %}
                {% include 'slide_cart_Divider' %}
            {% endcase %}
        </div>
    {% endfor %}



<div id="empty_cart_wrapper" style="position: fixed; bottom: 0;top: 0;  width: 100%; display: none;">
    <div class="empty_cart_elements_wrapper">
        <div id="empty_cart_img_wrapper" >
            <img class="slide_cart_img" src="{{ section.settings.empty_cart_image | img_url: 'large' }}" style="width: 30px;">
        </div>
        <div id="empty_cart_msg">
            {{ section.settings.empty_cart_text }}
        </div>
        <div id="empty_cart_shop_btn">
            <button onclick="window.location = '{{ shop.url | append: section.settings.cart_btn_link }}';">{{ section.settings.cart_btn_input }}</button>
        </div>
        <input class="sticky_lap" type="text" value="stick_lap_no">
        <input class="sticky_mob" type="text" value="stick_mob_no">
    </div>
</div>

<div id="loading_cart_wrapper" >
    <div>
        <input class="sticky_lap" type="text" value="stick_lap_no">
        <input class="sticky_mob" type="text" value="stick_mob_no">
    </div>
</div>

<div id="bt_sticked_blocks" style="position: fixed; bottom: 0; width: 100%;">
</div>

